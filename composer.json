  "description": "Markdown Editor for Wordpress",
  "keywords": [
    "wordpress",
    "markdown",
    "editor"
  ],
  "homepage": "http://hoducha.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Ha Ho",
      "email": "hoducha@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "lepture/editor",
